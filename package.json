  "license": "Apache-2.0",
  "licenseStart": "2017",
  "scripts": {
    "start": "tunnckocore-scripts",
    "extends": "@tunnckocore/config/scripts",
    "docs": "tunnckocore-scripts docs",
    "lint": "tunnckocore-scripts lint",
    "test": "tunnckocore-scripts test",
    "commit": "tunnckocore-scripts commit"
  },
  "engines": {
    "node": "^8.11.0 || >=10.13.0"
  },
  "dependencies": {
    "esm": "^3.0.84",
    "execa": "^1.0.0",
    "p-map-series": "^1.0.0",
    "split-cmd": "^1.0.0"
  },
  "devDependencies": {
    "@tunnckocore/config": "^0.5.1",
    "@tunnckocore/scripts": "^1.1.0",
    "asia": "^0.19.7"
  },
  "files": [
    "src",
    "index.js"
  ],
  "main": "index.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "version": "2.1.1",
  "repository": "tunnckoCoreLabs/execa",
  "homepage": "https://github.com/tunnckoCoreLabs/execa",
  "author": "Charlike Mike Reagent (https://tunnckocore.com)",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "renovate": {
